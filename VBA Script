Sub GrabFirstMailID()
    Worksheets("Sheet1").Range("A1:A1").Copy
End Sub
Sub MoveBgetouttheway()
    Worksheets("Sheet1").Range("B1:B1").Insert Shift:=xlDown
    Worksheets("Sheet1").Range("B1:B1").Value = Worksheets("Sheet1").Range("A1:A1").Value
    Worksheets("Sheet1").Range("A1:A1").Delete Shift:=xlUp
End Sub
